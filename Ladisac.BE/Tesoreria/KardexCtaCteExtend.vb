r        Try            Dim oVerificarDatos As New ErrorData            If Parametros Is Nothing Then                oVerificarDatos = VerificarDatos(Parametros)            Else                oVerificarDatos = VerificarDatos("DOC_FECHA_EMI_REF", "DOC_FECHA_VEN_REF", "CUC_ID_REF", "CCO_ID_REF", "CCC_ID_REF", "CCT_ID_REF", "TDO_ID_REF", "DTD_ID_REF", "DOC_SERIE_REF", "DOC_NUMERO_REF", "ITEM_REF", "MON_ID_CCC", "CCC_ID", "CCT_ID", "TDO_ID", "DTD_ID", "DOC_SERIE", "DOC_NUMERO", "ITEM", "MON_ID", "PER_ID_CLI", "CARGO", "ABONO", "DTE_CONTRAVALOR_DOC", "MPT_MEDIO_PAGO", "MPT_NUMERO_MEDIO", "PER_ID_BAN", "DOCUMENTO")            End If            If oVerificarDatos.NumeroError = 0 Then                vMensajeError = oVerificarDatos.MensajeGeneral                Return 0            Else                Return 1            End If        Catch ex As Exception            vMensajeError = ex.Message            Return 0        End Try    End FunctionEnd Class