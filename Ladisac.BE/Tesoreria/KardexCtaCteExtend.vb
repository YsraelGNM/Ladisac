NumeroError = 0                        VerificarDatos.MensajeError = mCodigo3                        VerificarDatos.Objeto = vCampos(elemento)                    End If                Case "DTD_ID_REF"                    If Len(DTD_ID_REF.Trim) = 3 Then                    Else                        VerificarDatos.NumeroError = 0                        VerificarDatos.MensajeError = mCodigo3                        VerificarDatos.Objeto = vCampos(elemento)                    End If                Case "DOC_SERIE_REF"                    If Len(DOC_SERIE_REF.Trim) = 3 Then                    Else                        VerificarDatos.NumeroError = 0                        VerificarDatos.MensajeError = mCodigo3                        VerificarDatos.Objeto = vCampos(elemento)                    End If                Case "DOC_NUMERO_REF"                    If Len(DOC_NUMERO_REF.Trim) > 0 Then                    Else                        VerificarDatos.NumeroError = 0                        VerificarDatos.MensajeError = mDescripcion                        VerificarDatos.Objeto = vCampos(elemento)                    End If                Case "ITEM_REF"                    If ITEM_REF >= 1 And ITEM_REF <= 9999 Then                    Else                        VerificarDatos.NumeroError = 0                        VerificarDatos.MensajeError = mDato                        VerificarDatos.Objeto = vCampos(elemento)                    End If                Case "MON_ID_CCC"                    If IsNothing(MON_ID_CCC) Then                    Else                        If Len(MON_ID_CCC.Trim) = 3 Then                        Else                            VerificarDatos.NumeroError = 0                            VerificarDatos.MensajeError = mCodigo3                            VerificarDatos.Objeto = vCampos(elemento)                        End If                    End If                Case "CCC_ID"                    If IsNoth