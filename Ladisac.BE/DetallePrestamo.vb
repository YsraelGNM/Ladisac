                    If Not Equals(TDO_ID, value.TDO_ID) OrElse Not Equals(DTD_ID, value.DTD_ID) OrElse Not Equals(CCC_ID, value.CCC_ID) OrElse Not Equals(DPR_SERIE, value.PRE_SERIE) OrElse Not Equals(DPR_NUMERO, value.PRE_NUMERO) Then
                        Throw New InvalidOperationException("El extremo principal de una relación de identificación solo se puede modificar cuando el extremo dependiente se encuentra en el estado agregado.")
                    End If
                End If
                Dim previousValue As Prestamo = _prestamo
                _prestamo = value
                FixupPrestamo(previousValue)
                OnNavigationPropertyChanged("Prestamo")
            End If
        End Set
    End Property

